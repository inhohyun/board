{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody class="table-group-divider">
    {{#articleList}}
        <tr>
            <th><a href="{{id}}">{{id}}</a></th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </tr>
    {{/articleList}}
    </tbody>

</table>
<a href="/articles/new" class="btn btn-primary">게시글 생성하기</a>
{{>layouts/footer}}